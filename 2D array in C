int main()
{
  
    int arr[3][2] = {10, 20, 30, 40, 50, 60,7};
    for (int i = 0; i < 3; i++)
    {
        for (int j = 0; j < 2; j++)
        {
           
           
           printf("%d ",arr[i][j]);
        }
   printf("\n"); 
}

}
